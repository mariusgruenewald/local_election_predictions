    "karlsruhe_spd_1 = get_page(page_link_1)\n",
    "karlsruhe_spd_2 = extract_names(karlsruhe_spd_1)\n",
    "karlsruhe_spd_3 = karlsruhe_spd_2[224:272].reset_index()\n",
    "karlsruhe_spd_4 = karlsruhe_spd_3['Name_total'].str.split(' ', expand=True)\n",
    "karlsruhe_spd_5 = karlsruhe_spd_4[3].str.split(r'([A-Z])', expand=True)\n",
    "karlsruhe_spd_5['last name'] = karlsruhe_spd_5[1] + karlsruhe_spd_5[2]\n",
    "karlsruhe_spd_5['job_1'] = karlsruhe_spd_5[3] + karlsruhe_spd_5[4]\n",
    "karlsruhe_spd_5['job_2'] = karlsruhe_spd_5[5] + karlsruhe_spd_5[6]\n",
    "karlsruhe_spd_6 = pd.concat([karlsruhe_spd_4, karlsruhe_spd_5], axis = 1)\n",
    "karlsruhe_spd_7 = karlsruhe_spd_4[2].str.split(r'([A-Z])', expand=True)\n",
    "karlsruhe_spd_7['place list'] = karlsruhe_spd_7[0]\n",
    "karlsruhe_spd_7['first name'] = karlsruhe_spd_7[1] + karlsruhe_spd_7[2]\n",
    "karlsruhe_spd_8 = pd.concat([karlsruhe_spd_6, karlsruhe_spd_7], axis = 1)\n",
    "karlsruhe_spd_9 = karlsruhe_spd_4[4].str.split(r'([A-Z])', expand=True)\n",
    "karlsruhe_spd_9['job_3'] = karlsruhe_spd_9[0]\n",
    "karlsruhe_spd_9['job_4'] = karlsruhe_spd_9[1] + karlsruhe_spd_9[2]\n",
    "karlsruhe_spd_9['job_5'] = karlsruhe_spd_9[3] + karlsruhe_spd_9[4]\n",
    "karlsruhe_spd_9['job_6'] = karlsruhe_spd_9[5] + karlsruhe_spd_9[6]\n",
    "karlsruhe_spd_10 = pd.concat([karlsruhe_spd_8, karlsruhe_spd_9], axis = 1)\n",
    "karlsruhe_spd_11 = karlsruhe_spd_4[5].str.split(r'([A-Z])', expand=True)\n",
