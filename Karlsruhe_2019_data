    "def split_data(data, sign):\n",
    "    \n",
    "    \"\"\" split string along '>' \n",
    "    -> generalize for any delimiter \"\"\"\n",
    "    data_new = data['Name_total'].apply(str)\n",
    "    data_new_2 = data_new.str.split('sign', expand = True)\n",
    "    \n",
    "    return data_new_2\n"
   ]
